$('#upd_user').click(function(){

	if($('#rec_id').val() != ''){
		$('#upd_user').hide();
		$('#pl_wait').show();

		$.ajax({
			type: 'POST',
			url:club_baseurl+'/upd_membership/',
			data: $('#frm_edit_mem').serialize(),
			success: function(res) {
				if(res)
					alert("Details are updated successfully!");
				else
					alert("Something went wrong!");
				//location.reload();
				$('#edit_mem_modal').modal('toggle');
				$('#mem_code_det').html('');
				$('#div_occr').hide();
				$('#num_occr').val('');
				$('#num_occr').hide('');
				$('#upd_user').show();
				$('#pl_wait').hide();
			}
		});
	}

});

